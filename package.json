{
  "name": "webdriverio-appium-mocha",
  "version": "1.0.0",
  "description": "Native app and web browser for android and ios using mocha with page object pattern",
  "scripts": {
    "ios": "wdio ./config/ios.config.js",
    "android": "wdio ./config/android.config.js",
    "iosBrowser": "wdio ./config/iosBrowser.config.js",
    "androidBrowser": "wdio ./config/androidBrowser.config.js",
    "lint": "eslint .",
    "allure": "allure generate --clean && allure open",
    "browserstack": "wdio ./test/config/browserstack.conf.js"
  },
  "keywords": [
    "Appium",
    "Mocha",
    "webdriverio",
    "wdio",
    "mobile automation",
    "native apps",
    "mobile browser"
  ],
  "author": "Rupak Mansingh",
  "devDependencies": {
    "@wdio/allure-reporter": "^6.1.14",
    "@wdio/appium-service": "^6.1.16",
    "@wdio/browserstack-service": "^6.8.0",
    "@wdio/cli": "^6.1.17",
    "@wdio/junit-reporter": "^6.8.1",
    "@wdio/local-runner": "^6.9.1",
    "@wdio/mocha-framework": "^6.8.0",
    "@wdio/spec-reporter": "^6.8.1",
    "@wdio/sync": "^6.9.1",
    "allure-commandline": "^2.13.0",
    "chromedriver": "^90.0.1",
    "eslint": "^7.3.0",
    "wdio-chromedriver-service": "^6.0.4"
  }
}
